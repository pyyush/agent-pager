{
  "name": "@agentpager/protocol",
  "version": "0.1.0",
  "description": "Shared types, Zod schemas, and risk classifier for AgentPager",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
